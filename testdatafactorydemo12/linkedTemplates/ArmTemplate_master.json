{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "testdatafactorydemo12"
		},
		"AzureTableStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureTableStorage1'"
		},
		"LS_ADLSConnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLSConnection'"
		},
		"LS_BlobConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BlobConnection'"
		},
		"LS_SQLConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQLConnection'"
		},
		"LS_ADLSConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://testazuredatalakegen2012.dfs.core.windows.net/"
		},
		"LS_HTTPConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseURL}"
		},
		"LS_RESTConnector_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseurl}"
		},
		"TR_EventTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/0b09d9a2-10b0-4bdb-966c-fa3ebfa3ba0a/resourceGroups/TestRG002/providers/Microsoft.Storage/storageAccounts/testblobstoragedemo012"
		},
		"AzureKeyVaultPE_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/0b09d9a2-10b0-4bdb-966c-fa3ebfa3ba0a/resourceGroups/TestRG002/providers/Microsoft.KeyVault/vaults/testkeyvaultdemo012"
		},
		"AzureKeyVaultPE_properties_groupId": {
			"type": "string",
			"defaultValue": "vault"
		},
		"AzureKeyVaultPE_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"testkeyvaultdemo012.vault.azure.net"
			]
		},
		"Blob_ManagedPE_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/0b09d9a2-10b0-4bdb-966c-fa3ebfa3ba0a/resourceGroups/TestRG002/providers/Microsoft.Storage/storageAccounts/testblobstoragedemo012"
		},
		"Blob_ManagedPE_properties_groupId": {
			"type": "string",
			"defaultValue": "blob"
		},
		"Blob_ManagedPE_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"testblobstoragedemo012.blob.core.windows.net"
			]
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "testdatafactorydemo12_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureTableStorage1_connectionString": {
						"value": "[parameters('AzureTableStorage1_connectionString')]"
					},
					"LS_ADLSConnection_accountKey": {
						"value": "[parameters('LS_ADLSConnection_accountKey')]"
					},
					"LS_SQLConnection_connectionString": {
						"value": "[parameters('LS_SQLConnection_connectionString')]"
					},
					"LS_ADLSConnection_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSConnection_properties_typeProperties_url')]"
					},
					"LS_HTTPConnection_properties_typeProperties_url": {
						"value": "[parameters('LS_HTTPConnection_properties_typeProperties_url')]"
					},
					"LS_RESTConnector_properties_typeProperties_url": {
						"value": "[parameters('LS_RESTConnector_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "testdatafactorydemo12_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_BlobConnection_connectionString": {
						"value": "[parameters('LS_BlobConnection_connectionString')]"
					},
					"AzureKeyVaultPE_properties_privateLinkResourceId": {
						"value": "[parameters('AzureKeyVaultPE_properties_privateLinkResourceId')]"
					},
					"AzureKeyVaultPE_properties_groupId": {
						"value": "[parameters('AzureKeyVaultPE_properties_groupId')]"
					},
					"AzureKeyVaultPE_properties_fqdns": {
						"value": "[parameters('AzureKeyVaultPE_properties_fqdns')]"
					},
					"Blob_ManagedPE_properties_privateLinkResourceId": {
						"value": "[parameters('Blob_ManagedPE_properties_privateLinkResourceId')]"
					},
					"Blob_ManagedPE_properties_groupId": {
						"value": "[parameters('Blob_ManagedPE_properties_groupId')]"
					},
					"Blob_ManagedPE_properties_fqdns": {
						"value": "[parameters('Blob_ManagedPE_properties_fqdns')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'testdatafactorydemo12_ArmTemplate_0')]"
			]
		},
		{
			"name": "testdatafactorydemo12_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"TR_EventTrigger_properties_typeProperties_scope": {
						"value": "[parameters('TR_EventTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'testdatafactorydemo12_ArmTemplate_1')]"
			]
		}
	]
}